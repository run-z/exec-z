{
  "name": "@run-z/exec-z",
  "version": "1.1.1",
  "description": "Abortable executions",
  "keywords": [
    "abortable",
    "cancelable",
    "promise"
  ],
  "homepage": "https://github.com/run-z/exec-z",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:run-z/exec-z.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/run-z/exec-z/issues"
  },
  "main": "dist/exec-z.js",
  "module": "dist/exec-z.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/exec-z.mjs",
      "require": "./dist/exec-z.js"
    }
  },
  "dependencies": {
    "@proc7ts/primitives": "^1.1.0"
  },
  "devDependencies": {
    "@proc7ts/eslint-config": "^2.0.5",
    "@proc7ts/rollup-helpers": "^1.0.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^12.12.62",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "codecov": "^3.7.2",
    "eslint": "^7.10.0",
    "eslint-plugin-jest": "^24.0.2",
    "gh-pages": "^3.1.0",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "rollup": "^2.28.2",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "run-z": "~1.0.2",
    "shx": "^0.3.2",
    "strip-ansi": "^6.0.0",
    "ts-jest": "^26.4.0",
    "tslib": "^2.0.1",
    "typedoc": "^0.19.0",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "bootstrap": "rollup --config ./rollup.config.js",
    "build": "run-z +z bootstrap",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf d.ts dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint .",
    "test": "run-z +z --then jest",
    "z": "run-z +bootstrap,+doc,+lint,+test"
  }
}
